  <DataSets>
    <DataSet Name="CoT_PL_ROWPermit_Conditions">
      <Query>
        <DataSourceName>CoTuscon</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PLPLANID">
            <Value>=Parameters!PLPLANID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT PC.PLANID, CC.[NAME] AS CONDITIONCATEGORY
	, C.[NAME] AS CONDITION, C.COMMENTS, C.[DESCRIPTION], C.ISENABLE
	, PC.CREATEDON, PC.ISSATISFIED, CS.SCOPENAME
	,CASE 
		WHEN CC.[NAME] LIKE 'Traffic Control%' THEN 1 
		WHEN CC.[NAME] LIKE 'General%' THEN 2 
		ELSE 5 
	END AS CategorySort
FROM PLPLANCONDITION PC
INNER JOIN CONDITION C ON PC.CONDITIONID = C.CONDITIONID
INNER JOIN CONDITIONCATEGORY CC ON C.CONDITIONCATEGORYID = CC.CONDITIONCATEGORYID
INNER JOIN CONDITIONSCOPE CS ON PC.SCOPE = CS.CONDITIONSCOPEID
WHERE PC.PLANID = @PLPLANID
ORDER BY CASE WHEN CC.[NAME] LIKE 'Traffic Control%' THEN 1 WHEN CC.[NAME] LIKE 'General%' THEN 2 ELSE 5 END, CC.[NAME]
</CommandText>
      </Query>
