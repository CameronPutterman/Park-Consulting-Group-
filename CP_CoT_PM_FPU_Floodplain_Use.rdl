  <Code>Function FormatPhoneNumber(ByVal phoneNumber As String) As String
If String.IsNullOrEmpty(phoneNumber) Then
        Return ""
End If
phoneNumber = System.Text.RegularExpressions.Regex.Replace(phoneNumber, "[^\d]", "")
If phoneNumber.Length = 10 Then
        Return String.Format("({0}) {1}-{2}", phoneNumber.Substring(0, 3), phoneNumber.Substring(3, 3), phoneNumber.Substring(6, 4))
Else
        Return "Invalid Phone Number"
    End If
End Function</Code>
