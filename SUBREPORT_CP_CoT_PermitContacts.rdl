  <DataSets>
    <DataSet Name="CoT_PM_PermitContacts">
      <Query>
        <DataSourceName>CoTuscon</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PMPERMITID">
            <Value>=Parameters!PMPERMITID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>;WITH CONTACT_CTE AS (
	SELECT GEMA.GLOBALENTITYID
		, MA.ADDRESSTYPE
		, RTRIM(LTRIM(COALESCE(MA.POBOX,'') + ' ')
			+ LTRIM(COALESCE(MA.ADDRESSLINE1,'') + ' ')
			+ LTRIM(COALESCE(MA.PREDIRECTION,'') + ' ') 
			+ LTRIM(COALESCE(MA.ADDRESSLINE2,'') + ' ') 
			+ LTRIM(COALESCE(MA.STREETTYPE,'') + ' ') 
			+ LTRIM(COALESCE(MA.POSTDIRECTION,'') + ' ') 
			+ LTRIM(COALESCE(MA.UNITORSUITE,'') + ' ') 
		  ) ADDRESS_LINE1
		, (CASE WHEN RTRIM(LTRIM(COALESCE(MA.CITY,''))) = '' 
			THEN '' 
			ELSE RTRIM(LTRIM(COALESCE(MA.CITY,''))) + ', ' END)
			+ (LTRIM(COALESCE(MA.[STATE],'') + ' ') 
			+ REPLACE((CASE WHEN LEN(REPLACE(COALESCE(MA.POSTALCODE,''),' ','')) &gt; 5 
			THEN LEFT(REPLACE(COALESCE(MA.POSTALCODE,''),' ',''),5) + '-' + RIGHT(REPLACE(COALESCE(MA.POSTALCODE,''),' ',''),ABS(LEN(REPLACE(COALESCE(MA.POSTALCODE,''),' ',''))-5)) 
			ELSE COALESCE(MA.POSTALCODE,'') END),'--','-')
		  ) ADDRESS_LINE2
	FROM GLOBALENTITYMAILINGADDRESS GEMA
	INNER JOIN MAILINGADDRESS MA ON GEMA.MAILINGADDRESSID = MA.MAILINGADDRESSID
)
SELECT PC.PMPERMITID
	, GE.GLOBALENTITYID
	, MA.ADDRESS_LINE1, MA.ADDRESS_LINE2
	, GE.FIRSTNAME + ' ' + GE.LASTNAME CONTACTNAME
	, GE.GLOBALENTITYNAME CONTACTCOMPANY
	, COALESCE(NULLIF(GE.BUSINESSPHONE, ''), NULLIF(GE.MOBILEPHONE, ''), NULLIF(GE.HOMEPHONE, ''), NULLIF(GE.OTHERPHONE, ''), '') CONTACTPHONE
	, LMCT.[NAME] CONTACTTYPE
	, ROW_NUMBER() OVER (
		ORDER BY
			CASE WHEN LMCT.[NAME] LIKE 'Applicant' THEN 1
				WHEN LMCT.[NAME] LIKE 'Owner' THEN 2
				WHEN LMCT.[NAME] LIKE 'Contractor' THEN 3
				WHEN LMCT.[NAME] LIKE 'Barricade Company' THEN 4
				WHEN LMCT.[NAME] LIKE 'Sub-Contractor' THEN 5
			ELSE 10
		END
	) AS ROWNBR
FROM PMPERMITCONTACT PC
INNER JOIN GLOBALENTITY GE ON GE.GLOBALENTITYID = PC.GLOBALENTITYID
INNER JOIN LANDMANAGEMENTCONTACTTYPE LMCT ON LMCT.LANDMANAGEMENTCONTACTTYPEID = PC.LANDMANAGEMENTCONTACTTYPEID
LEFT JOIN CONTACT_CTE MA ON MA.GLOBALENTITYID = GE.GLOBALENTITYID
WHERE PC.PMPERMITID = @PMPERMITID 
AND 
LMCT.[NAME] IN ( 'Applicant', 'Owner', 'Barricade Company', 'Contractor', 'Sub-Contractor' )</CommandText>
      </Query>
      <Fields>
        <Field Name="PMPERMITID">
          <DataField>PMPERMITID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="GLOBALENTITYID">
          <DataField>GLOBALENTITYID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ADDRESS_LINE1">
          <DataField>ADDRESS_LINE1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ADDRESS_LINE2">
          <DataField>ADDRESS_LINE2</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CONTACTNAME">
          <DataField>CONTACTNAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CONTACTCOMPANY">
          <DataField>CONTACTCOMPANY</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CONTACTPHONE">
          <DataField>CONTACTPHONE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CONTACTTYPE">
          <DataField>CONTACTTYPE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ROWNBR">
          <DataField>ROWNBR</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
